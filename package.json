{
  "name": "fauxid",
  "version": "0.1.0",
  "description": "一个基于Next.js和Supabase的虚拟身份管理应用，用于创建和管理合成身份信息",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "deploy:cloudflare": "CLOUDFLARE=true next build && wrangler pages publish .next/standalone --project-name=fauxid",
    "deploy:cloudflare:prod": "CLOUDFLARE=true next build && wrangler pages publish .next/standalone --project-name=fauxid --branch=main",
    "deploy:vercel": "vercel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h7ml/fauxid.git"
  },
  "keywords": [
    "nextjs",
    "supabase",
    "typescript",
    "identity-management",
    "tailwindcss",
    "shadcn-ui",
    "vercel"
  ],
  "homepage": "https://fauxid.vercel.app",
  "author": "h7ml",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toast": "^1.2.13",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@shadcn/ui": "^0.0.4",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "latest",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "next": "latest",
    "next-themes": "^0.4.3",
    "prettier": "^3.3.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.3",
    "tr46": "^5.1.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "19.0.2",
    "postcss": "8.4.49",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.7.2",
    "wrangler": "^4.14.4"
  }
}
