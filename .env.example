# Supabase配置
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# 应用配置
NEXT_PUBLIC_SITE_URL=https://your-project-name.pages.dev 

# NextAuth配置
NEXTAUTH_URL=https://your-project-name.pages.dev
NEXTAUTH_SECRET=your-nextauth-secret  # 生成命令: openssl rand -base64 32

# Linux.do OAuth配置
LINUX_DO_CLIENT_ID=your-linux-do-client-id
LINUX_DO_CLIENT_SECRET=your-linux-do-client-secret
LINUX_DO_REDIRECT_URI=https://your-project-name.pages.dev/api/auth/callback/linux-do

# Cloudflare标记
CLOUDFLARE=true 
